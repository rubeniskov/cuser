{
  "name": "@cuser/store",
  "version": "0.0.1",
  "description": "store tree changes managment",
  "main": "./configureStore",
  "scripts": {
    "test": "nyc ava -v",
    "coverage": "nyc report --reporter=lcov",
    "postcoverage": "codecov -F store",
    "docs": "typedoc --tsconfig ./tsconfig.json --out ./docs --plugin typedoc-plugin-markdown --mode file --namedAnchors --hideProjectName --publicPath docs/ --hideBreadcrumbs --excludeNotExported --excludeNotDocumented",
    "postdocs": "cat HEADER.md docs/globals.md > README.md",
    "types": "rm -rf types && tsc --declaration --emitDeclarationOnly --outDir types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "mutant-json": "0.6.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@cuser/proto": "^0.0.1",
    "@cuser/utils": "^0.0.1",
    "@cuser/validator": "^0.0.1",
    "ava": "^3.13.0",
    "codecov": "^3.8.1",
    "faker": "^5.1.0",
    "lorem-ipsum": "^2.0.3",
    "nyc": "^15.1.0",
    "traverse-json": "^0.5.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.11",
    "typescript": "4.1.2"
  },
  "peerDependencies": {
    "@cuser/proto": "^0.0.1",
    "@cuser/utils": "^0.0.1",
    "@cuser/validator": "^0.0.1"
  },
  "nyc": {
    "exclude": ["testing"]
  }
}
